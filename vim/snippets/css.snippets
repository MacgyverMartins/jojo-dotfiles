# {{{CSS SNIPPETS
####################################################################################################################################################################################################################################################################################
#
snippet reset
	html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent}
	body{line-height:1}
	article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}
	nav ul{list-style:none}
	blockquote,q{quotes:none}
	blockquote:before,blockquote:after,q:before,q:after{content:none}
	a{margin:0;padding:0;font-size:100%;vertical-align:baseline;background:transparent}
	ins{background-color:#ff9;color:#000;text-decoration:none}
	mark{background-color:#ff9;color:#000;font-style:italic;font-weight:bold}
	del{text-decoration:line-through}
	abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help}
	table{border-collapse:collapse;border-spacing:0}
	hr{display:block;height:1px;border:0;border-top:1px solid #ccc;margin:1em 0;padding:0}
	input,select{vertical-align:middle}
snippet a
	azimuth:${1:?}; ${2}
snippet bs
	box-shadow:${1:inset} ${2:0}px ${3:1}px ${4:0}px ${5:rgba(0,0,0,.3)}; ${6}
snippet bry
	-webkit-border-radius: ${1:5}px;
	-moz-border-radius: $1px;
	border-radius: $1px; 
snippet bra
	border-radius: ${1:5}px; 
snippet brtl
	border-top-left-radius: ${1:5}px; ${2}
snippet brtr
	border-top-right-radius: ${1:5}px; ${2}
snippet brbl
	border-bottom-left-radius: ${1:5}px; ${2}
snippet brbr
	border-bottom-right-radius: ${1:5}px; ${2}
snippet bl
	 border-left:${1:1}px ${2:solid} ${3:#ccc}; ${4}
snippet br
	border-right:${1:1}px ${2:solid} ${3:#ccc}; ${4}
snippet bb
	border-bottom:${1:1}px ${2:solid} ${3:#ccc}; ${4}
snippet bt
	border-top:${1:1}px ${2:solid} ${3:#ccc}; ${4}
snippet b
	border:${1:1}px ${2:solid} ${3:#ccc}; ${4}
snippet bg
	background:${1:#fff}; ${2}
snippet bga
	background:url('${1:../images/}') ${2:no-repeat} ${3:50%} ${4:50%} ${5:transparent}; ${6}
snippet bgg
	background-color: $1;
	background-repeat: repeat-x;
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(${1:COR1}), to(${2:COR2}));
	background: -webkit-linear-gradient(${3:top}, $1, $2);
	background: -moz-linear-gradient($3, $1, $2);
	background: -ms-linear-gradient($3, $1, $2);
	background: -o-linear-gradient($3, $1, $2);
	${4}
snippet bgi
	background-image: url('${1:../}'); ${2}
snippet bgr
	background-repeat: ${1:no-repeat}; ${2}
snippet bgc
	background-color:${1:#000}; ${2}
snippet bos
	box-sizing:${1:border-box}; ${2}
snippet c
	color:${1:#fc0}; ${2}
snippet co
	content:${1:''}; ${2}
snippet cl
	clear:${1:both}; ${2}
snippet cb
	clear:${1:both}; ${2}
snippet db
	display:block; ${1}
snippet dn
	display:none; ${1}
snippet di
	display:inline; ${1}
snippet fl
	float:left; ${1}
snippet fr
	float:right; ${1}
snippet fn
	float:none; ${1}
snippet fs
	font-size: ${1:13}px; ${2}
snippet fst
	font-style: ${1:italic}; ${2}
snippet ff
	font-family: "${1:Arial}", sans;
snippet fw
	font-weight:${1:bold}; ${2}
snippet h
	height:${1:20}px; ${2}
snippet lh
	line-height:${1:20}px; ${2}
#MARGINS
snippet m
	margin:${1:10}px; ${2}
snippet ma
	margin:${1:10}px ${2:10}px ${3:10}px ${4:10}px; ${5}
snippet mac
	margin:${1:10px} ${2:auto}; ${3}
snippet mt
	margin-top:${1:10}px; ${2}
snippet mr
	margin-right:${1:10}px; ${2}
snippet mb
	margin-bottom:${1:10}px; ${2}
snippet ml
	margin-left:${1:10}px; ${2}
snippet ov
	overflow:${1:visible}; ${2}
snippet oh
	overflow:${1:hidden}; ${2}
snippet ox
	overflow-x:${1:hidden}; ${2}
snippet oy
	overflow-y:${1:hidden}; ${2}
snippet ou
	outline:${1:1px} ${2:solid} ${3:#fc0}; ${4}
#PADDING
snippet p
	padding:${1:10}px; ${2}
snippet pa
	padding:${1:10}px ${2:10}px ${3:10}px ${4:10}px; ${5}
snippet pt
	padding-top:${1:10}px; ${2}
snippet pr
	padding-right:${1:10}px; ${2}
snippet pb
	padding-bottom:${1:10}px; ${2}
snippet pl
	padding-left:${1:10}px; ${2}
snippet poa
	position:${1:absolute}; ${2}
snippet pof
	position:${1:fixed}; ${2}
snippet po
	position:${1:relative}; ${2}
snippet t
	top:${1:0}px; ${2}
snippet r
	right:${1:0}px; ${2}
snippet bo
	bottom:${1:0}px; ${2}
snippet l
	left:${1:0}px; ${2}
snippet transition
	-webkit-transition: .${1:1}s ${2:all} ${3:ease}; 
	-moz-transition: .$1s $2 $3;
	-o-transition: .$1s $2 $3;
	transition: .$1s $2 $3; ${4}
snippet ts
	text-shadow:${1:0}px ${2:1}px ${3} ${4:#ccc}; ${5}
snippet td
	text-decoration:${1:none}; ${2}
snippet ta
	text-align:${1:center}; ${2}
snippet tet
	text-transform:${1:capitalize}; ${2}
snippet ti
	text-indent:${1:-9999}px; ${2}
snippet va
	vertical-align:${1:middle}; ${2}
snippet w
	width:${1:20}px; ${2}
snippet zi
	z-index:${1:1}; ${2}
#
#}}}-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
